{
  "mcpServers": {
    "mariadb-db-agents": {
      "command": "python",
      "args": [
        "-m",
        "mariadb_db_agents.mcp_server.main"
      ],
      "env": {
        "OPENAI_API_KEY": "your-openai-api-key-here",
        "DB_HOST": "your-db-host",
        "DB_PORT": "3306",
        "DB_USER": "your-db-user",
        "DB_PASSWORD": "your-db-password",
        "DB_DATABASE": "your-database-name"
      }
    }
  }
}

